#teste número primo
n = eval(input("insira um número para saber se é primo "))
if n >=1:
    for x in range(1,n+1):
        if n%x != 0:
            print(n, " é primo")
            break
        else:
            print(n, " não é primo")
            break
elif n==0:
    print("n é zero")
else:
    print(n," é negativo")
